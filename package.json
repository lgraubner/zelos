    "build": "webpack --mode production"
  },
  "keywords": [],
  "author": "Lars Graubner <mail@larsgraubner.de> (https://larsgraubner.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "eslint": "4.19.1",
    "eslint-plugin-flowtype": "2.46.1",
    "flow-babel-webpack-plugin": "1.1.1",
    "husky": "0.14.3",
