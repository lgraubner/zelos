{
  "name": "zelos",
  "version": "1.0.0",
  "description": "Opinionated static site generator",
  "bin": "dist/zelos.js",
  "scripts": {
    "develop": "webpack -w --mode development",
    "build": "webpack --mode production"
  },
  "keywords": [],
  "author": "Lars Graubner <mail@larsgraubner.de> (https://larsgraubner.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.44",
    "@babel/plugin-transform-flow-strip-types": "7.0.0-beta.44",
    "babel-eslint": "8.2.3",
    "babel-loader": "8.0.0-beta.2",
    "browser-sync": "2.23.6",
    "chalk": "2.4.0",
    "debug": "3.1.0",
    "eslint": "4.19.1",
    "eslint-plugin-flowtype": "2.46.2",
    "flow-babel-webpack-plugin": "1.1.1",
    "front-matter": "2.3.0",
    "frontmatter": "0.0.3",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "handlebars": "4.0.11",
    "highlight.js": "9.12.0",
    "html-minifier": "3.5.15",
    "husky": "0.14.3",
    "install": "0.11.0",
    "lint-staged": "7.0.4",
    "lodash": "4.17.5",
    "marked": "0.3.19",
    "mri": "1.1.0",
    "nanoseconds": "1.0.1",
    "ora": "2.0.0",
    "pluralize": "7.0.0",
    "prettier": "1.12.1",
    "pretty-ms": "3.1.0",
    "rss": "1.2.2",
    "serve": "6.5.5",
    "sw-precache": "5.2.1",
    "update-notifier": "2.5.0",
    "waait": "1.0.1",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-node-externals": "1.7.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-flow-strip-types",
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "script"
    },
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-console": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "@babel/preset-env": "7.0.0-beta.44"
  }
}
